@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<http://color.com#Blue> rdfs:label "Blue (label)"@en .

<http://color.com#ColorPalette> a rdfs:Class, sh:NodeShape ;
  sh:name "Color palette"@en ;
  sh:property [
    sh:in (<http://color.com#Red> <http://color.com#Green> <http://color.com#Blue> <http://color.com#Yellow> <http://color.com#Pink>)
  ] ;
  sh:targetClass sh:ColorPalette .

<http://color.com#Green> rdfs:label "Green (label)"@en .

<http://color.com#Pink> rdfs:label "Pink (label)"@en .

<http://color.com#Red> rdfs:label "Red (label)"@en .

<http://color.com#Yellow> rdfs:label "Yellow (label)"@en .
