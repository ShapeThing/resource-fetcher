@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix c: <http://color.com#> .

c:ColorPalette a rdfs:Class, sh:NodeShape ;
  sh:targetClass sh:ColorPalette ;
  sh:name "Color palette"@en ;
  sh:property [
    sh:name "Colors"@en ;
    sh:path sh:color ;
    sh:in (c:Red c:Green c:Blue) ;
  ] ;
.

c:Red rdfs:label "Red (label)"@en .
c:Green rdfs:label "Green (label)"@en .
c:Blue rdfs:label "Blue (label)"@en .