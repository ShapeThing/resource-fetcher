{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "always",
    "source.fixAll.eslint": "always"
  },
  "deno.enable": true,
  "deno.lint": true,
  "editor.formatOnSave": true,
  "cSpell.words": ["Grapoi"],
  "[typescript]": { "editor.defaultFormatter": "denoland.vscode-deno" }
}
