@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix ns0: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

foaf:Person a rdfs:Class, owl:Class, sh:NodeShape ;
  rdfs:comment "A person." ;
  rdfs:isDefinedBy foaf: ;
  rdfs:label "Person" ;
  rdfs:subClassOf geo:SpatialThing, foaf:Agent ;
  owl:disjointWith foaf:Organization, foaf:Project ;
  owl:equivalentClass schema:Person, <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
  ns0:term_status "stable" ;
  sh:property foaf:Person_currentProject, foaf:Person_family_name, foaf:Person_familyName, foaf:Person_firstName, foaf:Person_geekcode, foaf:Person_img, foaf:Person_knows, foaf:Person_lastName, foaf:Person_myersBriggs, foaf:Person_pastProject, foaf:Person_plan, foaf:Person_publications, foaf:Person_schoolHomepage, foaf:Person_surname, foaf:Person_workInfoHomepage, foaf:Person_workplaceHomepage .

foaf:Person_currentProject a sh:PropertyShape ;
  sh:class owl:Thing ;
  sh:path foaf:currentProject .

foaf:Person_family_name a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:family_name .

foaf:Person_familyName a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:familyName .

foaf:Person_firstName a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:firstName .

foaf:Person_geekcode a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:geekcode .

foaf:Person_img a sh:PropertyShape ;
  sh:class foaf:Image ;
  sh:path foaf:img .

foaf:Person_knows a sh:PropertyShape ;
  sh:class foaf:Person ;
  sh:path foaf:knows .

foaf:Person_lastName a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:lastName .

foaf:Person_myersBriggs a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:myersBriggs .

foaf:Person_pastProject a sh:PropertyShape ;
  sh:class owl:Thing ;
  sh:path foaf:pastProject .

foaf:Person_plan a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:plan .

foaf:Person_publications a sh:PropertyShape ;
  sh:class foaf:Document ;
  sh:path foaf:publications .

foaf:Person_schoolHomepage a sh:PropertyShape ;
  sh:class foaf:Document ;
  sh:path foaf:schoolHomepage .

foaf:Person_surname a sh:PropertyShape ;
  sh:nodeKind sh:Literal ;
  sh:path foaf:surname .

foaf:Person_workInfoHomepage a sh:PropertyShape ;
  sh:class foaf:Document ;
  sh:path foaf:workInfoHomepage .

foaf:Person_workplaceHomepage a sh:PropertyShape ;
  sh:class foaf:Document ;
  sh:path foaf:workplaceHomepage .
