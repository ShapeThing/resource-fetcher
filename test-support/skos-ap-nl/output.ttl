@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skosapnl: <http://nlbegrip.nl/def/skosapnl#> .
@prefix isothes: <http://purl.org/iso25964/skos-thes#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

skosapnl:Concept a sh:NodeShape ;
  rdfs:label "Begrip"@nl, "Concept"@en ;
  rdfs:comment "Een abstract idee of notie."@nl, "An idea or notion; a unit of thought."@en ;
  sh:property skosapnl:Concept-example, skosapnl:Concept-comment, skosapnl:Concept-source, skosapnl:Concept-hiddenLabel, skosapnl:Concept-related, skosapnl:Concept-notation, skosapnl:Concept-broader, skosapnl:Concept-narrower, skosapnl:Concept-inScheme, skosapnl:Concept-topConceptOf, skosapnl:Concept-closeMatch, skosapnl:Concept-label, skosapnl:Concept-editorialNote, skosapnl:Concept-prefLabel, skosapnl:Concept-historyNote, skosapnl:Concept-altLabel, skosapnl:Concept-changeNote, skosapnl:Concept-scopeNote, skosapnl:Concept-definition, skosapnl:Concept-narrowerInstantial, skosapnl:Concept-exactMatch, skosapnl:Concept-narrowMatch, skosapnl:Concept-relatedMatch, skosapnl:Concept-narrowerPartitive, skosapnl:Concept-broaderPartitive, skosapnl:Concept-broaderGeneric, skosapnl:Concept-broaderInstantial, skosapnl:Concept-narrowerGeneric, skosapnl:Concept-broadMatch ;
  sh:sparql skosapnl:OrphanConcept, skosapnl:TopConceptHavingBroader, skosapnl:UnambiguousNotations, skosapnl:UniquePreflabelWithinConceptScheme ;
  sh:targetClass skos:Concept .

skosapnl:Concept-example a sh:PropertyShape ;
  rdfs:label "voorbeeld"@nl ;
  sh:order 35 ;
  sh:description "An example of the use of a concept."@en, "Een voorbeeld van het gebruik van het concept."@nl ;
  sh:group skosapnl:NotesPropertyGroup ;
  sh:name "voorbeeld"@nl, "example"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:example ;
  sh:or ([
    sh:datatype rdf:langString
  ] [
    sh:datatype xsd:string
  ]) .

skosapnl:Concept-comment a sh:PropertyShape ;
  rdfs:label "comment"@nl ;
  sh:order 5 ;
  sh:description "A description of the subject resource in simple language, at language level B1, which can be understood without further context."@en, "Een beschrijving van iets in eenvoudige taal, op taalniveau B1, die zonder verdere context begrepen kan worden."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "comment"@en, "uitleg"@nl ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path rdfs:comment ;
  sh:datatype rdf:langString .

skosapnl:Concept-source a sh:PropertyShape ;
  rdfs:label "bron"@nl ;
  sh:order 11 ;
  sh:description "Relateert een begrip aan een brondocument waarvan het is afgeleid."@nl, "Relates a concept to a document from which it is derived."@en ;
  sh:group skosapnl:DefinitionPropertyGroup ;
  sh:name "heeft bron"@nl, "source"@en ;
  sh:nodeKind sh:BlankNodeOrIRI ;
  sh:severity sh:Warning ;
  sh:path dct:source ;
  sh:node skosapnl:SourceDocument .

skosapnl:Concept-hiddenLabel a sh:PropertyShape ;
  rdfs:label "zoekterm"@nl ;
  sh:order 2 ;
  sh:description "A hidden label is a lexical label for a resource that should be hidden when generating visual displays of the resource, but should still be accessible to free text search operations."@en, "Een zoekterm is een naam waarmee de vindbaarheid van een resource wordt vergroot, maar verder verborgen blijft."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "zoekterm"@nl, "hidden label"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:hiddenLabel ;
  sh:datatype rdf:langString ;
  sh:disjoint skos:altLabel, skos:prefLabel .

skosapnl:Concept-related a sh:PropertyShape ;
  rdfs:label "is gerelateerd aan"@nl ;
  sh:order 22 ;
  sh:description "Een concept met een associatieve semantische relatie"@nl, "Relates a concept to a concept with which there is an associative semantic relationship."@en ;
  sh:group skosapnl:SemanticRelationPropertyGroup ;
  sh:name "is gerelateerd aan"@nl, "has related"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:related ;
  sh:disjoint skos:broader, skos:narrower ;
  sh:class skos:Concept .

skosapnl:Concept-notation a sh:PropertyShape ;
  rdfs:label "Code"@nl ;
  sh:order 3 ;
  sh:description "A notation, also known as classification code, is a string of characters used to uniquely identify a concept within the scope of a given concept scheme."@en, "Een code voor een begrip is een tekenreeks ter aanduiding van precies één begrip uit een begrippenkader."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "code"@nl, "notation"@en ;
  sh:severity sh:Warning ;
  sh:path skos:notation .

skosapnl:Concept-broader a sh:PropertyShape ;
  rdfs:label "heeft bovenliggend begrip"@nl ;
  sh:order 20 ;
  sh:description "Relateert een begrip aan een bovenliggend begrip in de hierarchie."@nl, "Relates a concept to a concept that is more general in meaning."@en ;
  sh:group skosapnl:SemanticRelationPropertyGroup ;
  sh:name "heeft bovenliggend begrip"@nl, "has broader"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:broader ;
  sh:disjoint skos:related ;
  sh:class skos:Concept .

skosapnl:Concept-narrower a sh:PropertyShape ;
  rdfs:label "heeft onderliggend begrip"@nl ;
  sh:order 21 ;
  sh:description "Relateert een begrip aan een onderliggend begrip in de hierarchie."@nl, "Relates a concept to a concept that is more specific in meaning."@en ;
  sh:group skosapnl:SemanticRelationPropertyGroup ;
  sh:name "heeft onderliggend begrip"@nl, "has narrower"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:narrower ;
  sh:disjoint skos:related ;
  sh:class skos:Concept .

skosapnl:Concept-inScheme a sh:PropertyShape ;
  sh:order 6 ;
  sh:description "Relateert een begrip aan een begrippenkader waarin het is opgenomen."@nl, "Relates a resource (for example a concept) to a concept scheme in which it is included."@en ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "in kader"@nl, "in scheme"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Violation ;
  sh:path skos:inScheme ;
  sh:class skos:ConceptScheme .

skosapnl:Concept-topConceptOf a sh:PropertyShape ;
  rdfs:label "is topbegrip van"@nl ;
  sh:order 23 ;
  sh:description "Relateert een begrip aan het begrippenkader waar het een top begrip van is."@nl, "Relates a concept to the concept scheme that it is a top level concept of."@en ;
  sh:group skosapnl:SemanticRelationPropertyGroup ;
  sh:name "is topbegrip van"@nl, "is top concept in scheme"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:topConceptOf ;
  sh:class skos:ConceptScheme .

skosapnl:Concept-closeMatch a sh:PropertyShape ;
  rdfs:label "is vrijwel overeenkomstig"@nl ;
  sh:order 42 ;
  sh:description "Relates two concepts that are sufficiently similar that they can be used interchangeably in some information retrieval applications."@en, "Relateert twee begrippen uit verschillende begrippenkaders die zoveel op elkaar lijken dat ze in sommige toepassingen door elkaar kunnen worden gebruikt voor het ophalen van informatie. De betekenis van deze begrippen is ongeveer gelijk aan elkaar."@nl ;
  sh:group skosapnl:MatchingRelationshipsPropertyGroup ;
  sh:name "is vrijwel overeenkomstig"@nl, "has close match"@en ;
  sh:nodeKind sh:IRI ;
  sh:path skos:closeMatch ;
  sh:disjoint skos:narrowMatch, skos:exactMatch, skos:relatedMatch ;
  sh:class skos:Concept .

skosapnl:Concept-label a sh:PropertyShape ;
  rdfs:label "naam"@nl ;
  sh:order 4 ;
  sh:description "Een naam is een mensleesbare term ter aanduiding van het begrip."@nl, "A word or phrase used to label the concept."@en ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "naam"@nl, "label"@en ;
  sh:nodeKind sh:Literal ;
  sh:path rdfs:label ;
  sh:datatype rdf:langString .

skosapnl:Concept-editorialNote a sh:PropertyShape ;
  rdfs:label "redactionele notitie"@nl ;
  sh:order 32 ;
  sh:description "A note for an editor, translator or maintainer of the vocabulary."@en, "Een notitie voor de redacteur, vertaler of beheerder van het begrippenkader."@nl ;
  sh:group skosapnl:NotesPropertyGroup ;
  sh:name "redactionele notitie"@nl, "editorial note"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:editorialNote ;
  sh:datatype rdf:langString .

skosapnl:Concept-prefLabel rdfs:label "Voorkeursterm"@nl ;
  sh:order 0 ;
  sh:description "The preferred lexical label for a resource, in a given language."@en, "Een naam die in een specifieke taal de geprefereerde lexicale naam is."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "voorkeursterm"@nl, "preferred label"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Violation ;
  sh:path skos:prefLabel ;
  sh:datatype rdf:langString ;
  sh:disjoint skos:altLabel ;
  sh:minCount 1 ;
  sh:uniqueLang true .

skosapnl:Concept-historyNote a sh:PropertyShape ;
  rdfs:label "historie notitie"@nl ;
  sh:order 33 ;
  sh:description "A note about the past state/use/meaning of a concept."@en, "Notitie over de voormalige status/gebruik/betekenis van het begrip"@nl ;
  sh:group skosapnl:NotesPropertyGroup ;
  sh:name "historie notitie"@nl, "history note"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:historyNote ;
  sh:datatype rdf:langString .

skosapnl:Concept-altLabel a sh:PropertyShape ;
  rdfs:label "alternatieve term"@nl ;
  sh:order 1 ;
  sh:description "An alternative lexical label for a resource."@en, "Een alternatieve term voor een begrip is een term die gebruikt kan worden als alternatief voor de voorkeursterm van dit begrip, maar niet de voorkeur heeft."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "alternatieve term"@nl, "alternative name"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:altLabel ;
  sh:datatype rdf:langString ;
  sh:disjoint skos:hiddenLabel, skos:prefLabel .

skosapnl:Concept-changeNote a sh:PropertyShape ;
  rdfs:label "wijzigingsnotitie"@nl ;
  sh:order 31 ;
  sh:description "A note about a modification to a concept."@en, "Notitie over een wijziging aan het concept"@nl ;
  sh:group skosapnl:NotesPropertyGroup ;
  sh:name "wijzigingsnotitie"@nl, "change note"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:changeNote ;
  sh:datatype rdf:langString .

skosapnl:Concept-scopeNote a sh:PropertyShape ;
  rdfs:label "toelichting"@nl ;
  sh:order 34 ;
  sh:description "A note that helps to clarify the meaning and/or the use of a concept."@en, "Een notitie die meer duidelijkheid geeft over de betekenis of gebruik van een begrip."@nl ;
  sh:group skosapnl:NotesPropertyGroup ;
  sh:name "toelichting"@nl, "scope note"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path skos:scopeNote ;
  sh:datatype rdf:langString .

skosapnl:Concept-definition a sh:PropertyShape ;
  rdfs:label "definitie"@nl ;
  sh:order 10 ;
  sh:description "Een definitie van een begrip is een tekstuele beschrijving van de betekenis van dit begrip waarmee het begrip kan worden onderscheiden van andere begrippen."@nl, "A statement or formal explanation of the meaning of a concept."@en ;
  sh:group skosapnl:DefinitionPropertyGroup ;
  sh:name "definitie"@nl, "definition"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Violation ;
  sh:path skos:definition ;
  sh:datatype rdf:langString ;
  sh:minCount 1 .

skosapnl:Concept-narrowerInstantial a sh:PropertyShape ;
  rdfs:label "is categorie van"@nl ;
  sh:order 56 ;
  sh:description "Relateert een begrip, de categorie, aan een begrip dat exemplarisch is voor eerstgenoemde begrip."@nl, "Used to state the immediate (direct or one-step) class - instance relationship."@en ;
  sh:group skosapnl:Iso-thesRelationships ;
  sh:name "is categorie van"@nl, "narrower term (instantial)"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path isothes:narrowerInstantial ;
  sh:class skos:Concept .

skosapnl:Concept-exactMatch a sh:PropertyShape ;
  rdfs:label "is exact overeenkomstig"@nl ;
  sh:order 43 ;
  sh:description "Used to link two concepts, indicating a high degree of confidence that the concepts can be used interchangeably across a wide range of information retrieval applications. Exact match is a transitive property."@en, "Relateert een begrip aan een overeenkomstig begrip uit een ander begrippenkader, waarbij de betekenis van deze begrippen bij uiteenlopende toepassingen gelijk aan elkaar kan worden verondersteld."@nl ;
  sh:group skosapnl:MatchingRelationshipsPropertyGroup ;
  sh:name "is exact overeenkomstig"@nl, "has exact match"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:exactMatch ;
  sh:disjoint skos:closeMatch, skos:narrowMatch, skos:relatedMatch, skos:broadMatch ;
  sh:class skos:Concept .

skosapnl:Concept-narrowMatch a sh:PropertyShape ;
  rdfs:label "heeft overeenkomstig onderliggend"@nl ;
  sh:order 41 ;
  sh:description "Used to state a hierarchical mapping link between two conceptual resources in different concept schemes."@en, "Relateert een begrip aan een specifieker begrip uit een ander begrippenkader."@nl ;
  sh:group skosapnl:MatchingRelationshipsPropertyGroup ;
  sh:name "heeft overeenkomstig onderliggend"@nl, "has narrower match"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:narrowMatch ;
  sh:disjoint skos:closeMatch, skos:exactMatch, skos:relatedMatch, skos:broadMatch ;
  sh:class skos:Concept .

skosapnl:Concept-relatedMatch a sh:PropertyShape ;
  rdfs:label "is overeenkomstig verwant"@nl ;
  sh:order 44 ;
  sh:description "Used to state an associative mapping link between two conceptual resources in different concept schemes."@en, "Relateert een begrip aan een verwant begrip uit een ander begrippenkader."@nl ;
  sh:group skosapnl:MatchingRelationshipsPropertyGroup ;
  sh:name "is overeenkomstig verwant"@nl, "has related match"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:relatedMatch ;
  sh:disjoint skos:closeMatch, skos:narrowMatch, skos:exactMatch, skos:broadMatch ;
  sh:class skos:Concept .

skosapnl:Concept-narrowerPartitive a sh:PropertyShape ;
  rdfs:label "omvat"@nl ;
  sh:order 54 ;
  sh:description "Relateert een begrip aan een minder omvattend begrip."@nl, "Relates a concept to a less including concept."@en ;
  sh:group skosapnl:Iso-thesRelationships ;
  sh:name "omvat"@nl, "narrower term (partitive)"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path isothes:narrowerPartitive ;
  sh:class skos:Concept .

skosapnl:Concept-broaderPartitive a sh:PropertyShape ;
  sh:order 53 ;
  sh:description "Relateert een begrip aan een meer omvattend begrip."@nl, "Relates a concept to a more including concept."@en ;
  sh:group skosapnl:Iso-thesRelationships ;
  sh:name "is onderdeel van"@nl, "broader term (partitive)"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path isothes:broaderPartitive ;
  sh:class skos:Concept .

skosapnl:Concept-broaderGeneric a sh:PropertyShape ;
  rdfs:label "Is specialisatie van"@nl ;
  sh:order 51 ;
  sh:description "Relateert een begrip aan een meer generiek begrip."@nl, "Used to state the immediate (direct or one-step) class - generalized class relationship."@en ;
  sh:group skosapnl:Iso-thesRelationships ;
  sh:name "is specialisatie van"@nl, "broader term (generic)"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path isothes:broaderGeneric ;
  sh:class skos:Concept .

skosapnl:Concept-broaderInstantial a sh:PropertyShape ;
  rdfs:label "is exemplaar van"@nl ;
  sh:order 55 ;
  sh:description "Relateert een begrip, een exemplaar, aan een ander begrip, de categorie, waartoe eerstgenoemde begrip behoort."@nl, "Used to state the immediate (direct or one-step) instance - class relationship."@en ;
  sh:group skosapnl:Iso-thesRelationships ;
  sh:name "is exemplaar van"@nl, "broader term (instantial)"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path isothes:broaderInstantial ;
  sh:class skos:Concept .

skosapnl:Concept-narrowerGeneric a sh:PropertyShape ;
  rdfs:label "Is generalisatie van"@nl ;
  sh:order 52 ;
  sh:description "Relateert een begrip aan een meer specifieker begrip"@nl, "Related a concept to a more specific concept"@en ;
  sh:group skosapnl:Iso-thesRelationships ;
  sh:name "is generalisatie van"@nl, "is generalisation of"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path isothes:narrowerGeneric ;
  sh:class skos:Concept .

skosapnl:Concept-broadMatch a sh:PropertyShape ;
  rdfs:label "heeft overeenkomstig bovenliggend"@nl ;
  sh:order 40 ;
  sh:description "Used to state a hierarchical mapping link between two conceptual resources in different concept schemes."@en, "skos:broadMatch is used to state a hierarchical mapping link between two conceptual resources in different concept schemes."@en ;
  sh:group skosapnl:MatchingRelationshipsPropertyGroup ;
  sh:name "heeft overeenkomstig bovenliggend"@nl, "has broader match"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path skos:broadMatch ;
  sh:disjoint skos:closeMatch, skos:narrowMatch, skos:exactMatch, skos:relatedMatch ;
  sh:class skos:Concept .

skosapnl:NotesPropertyGroup a sh:PropertyGroup ;
  rdfs:label "Notes"@en, "Notities"@nl ;
  sh:order 3 .

skosapnl:LabelsAndDescriptionPropertyGroup a sh:PropertyGroup ;
  rdfs:label "Labels and Description"@en, "Termen en beschrijvingen"@nl ;
  sh:order 0 .

skosapnl:DefinitionPropertyGroup a sh:PropertyGroup ;
  rdfs:label "Definition"@en, "Definitie"@nl ;
  sh:order 1 .

skosapnl:SourceDocument a sh:NodeShape ;
  rdfs:label "Brondocument"@nl, "Source document"@en ;
  rdfs:comment "Een brondocument is een resource op het web of een fysiek document dat relevant is voor een begrip."@nl, "A source document is a document or web resource that is relevant for a concept"@en ;
  sh:property skosapnl:SourceDocument-label, skosapnl:SourceDocument-comment, skosapnl:SourceDocument-page, skosapnl:SourceDocument-bibliographicCitation ;
  sh:targetObjectsOf dct:source .

skosapnl:SemanticRelationPropertyGroup a sh:PropertyGroup ;
  rdfs:label "Semantic Relationships"@en, "Semantische relaties"@nl ;
  sh:order 2 .

skosapnl:MatchingRelationshipsPropertyGroup a sh:PropertyGroup ;
  rdfs:label "Matching Relationships"@en, "Harmonisatierelaties"@nl ;
  sh:order 4 .

skosapnl:Iso-thesRelationships a sh:PropertyGroup ;
  rdfs:label "Iso-thes Relationships"@en, "Iso-thes relaties"@nl ;
  sh:order 5 .

skosapnl:SourceDocument-label a sh:PropertyShape ;
  rdfs:label "naam"@nl ;
  sh:order 4 ;
  sh:description "Een naam is een voor mensen leesbare term ter aanduiding van het brondocument."@nl, "A word or phrase used to label the source document."@en ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "naam"@nl, "label"@en ;
  sh:severity sh:Info ;
  sh:path dct:title ;
  sh:datatype rdf:langString ;
  sh:minCount 1 .

skosapnl:SourceDocument-comment a sh:PropertyShape ;
  rdfs:label "uitleg"@nl ;
  sh:order 5 ;
  sh:description "Een beschrijving van het brondocument in eenvoudige taal, op taalniveau B1, die zonder verdere context begrepen kan worden."@nl, "A description of the source document in simple language, at language level B1, which can be understood without further context."@en ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "comment"@en, "uitleg"@nl ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path rdfs:comment ;
  sh:datatype rdf:langString .

skosapnl:SourceDocument-page a sh:PropertyShape ;
  rdfs:label "url"@nl ;
  sh:order 7 ;
  sh:description "Vindplaats van de bron op internet."@nl, "Location of the source data on the internet"@en ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "url"@nl, "page"@en ;
  sh:nodeKind sh:IRI ;
  sh:severity sh:Warning ;
  sh:path foaf:page .

skosapnl:SourceDocument-bibliographicCitation a sh:PropertyShape ;
  rdfs:label "bronverwijzing"@nl ;
  sh:order 8 ;
  sh:description "Een bibliografische referentie naar het brondocument."@nl, "A bibliographic reference for the resource."@en ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "bronverwijzing"@nl, "bibliographic citation"@en ;
  sh:nodeKind sh:Literal ;
  sh:severity sh:Warning ;
  sh:path dct:bibliographicCitation ;
  sh:datatype xsd:string .