<>
  a sh:NodeShape ;
  sh:targetClass sh:NodeShape ;
  sh:name "Shape"@en, "Vorm"@nl ;

  # Fetch all things normally and for groups fetch their contents.
  sh:property [
    sh:path sh:property ;

    sh:property [
      sh:path sh:group ;
      sh:property [ sh:path sh:name ] ;
      sh:property [ sh:path rdf:type ] ;
      sh:property [ sh:path sh:order ] ;
      sh:node <> ;
    ] ;
  ] ;
